<div>
    {{#users}}
        <h1>User</h1>
        <h3> name:  {{name}}</h3>
        <h3> email: {{email}} </h3>
        {{#allowed}}
            <form action="/follow" method="post">
                <input hidden name="userEmail" value={{userEmail}}>
                <input hidden name="password" value={{passwordUser}}>
                <input hidden name="followerEmail" value={{email}}>
                <input value="follow" type="submit"/>
            </form>
        {{/allowed}}
    {{/users}}
</div>